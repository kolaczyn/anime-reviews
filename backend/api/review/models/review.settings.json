{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "name": "review",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "subtitle": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "story": {
      "type": "integer",
      "required": true,
      "max": 100,
      "min": 0
    },
    "opening": {
      "type": "integer",
      "required": true,
      "max": 100,
      "min": 0
    },
    "characters": {
      "type": "integer",
      "required": true,
      "max": 100,
      "min": 0
    },
    "awesomeness": {
      "type": "integer",
      "required": true,
      "max": 100,
      "min": 0
    },
    "background": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "imageSmall": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "similarReviewOne": {
      "model": "review"
    },
    "pageContent": {
      "type": "dynamiczone",
      "components": [
        "review.no-spoiler",
        "review.spoiler-alert"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    }
  }
}
